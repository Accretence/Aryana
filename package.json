{
  "name": "aryana",
  "version": "0.0.8",
  "description": "Next.js UI layout & components built upon geist-ui.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Accretence/Aryana.git"
  },
  "keywords": [
    "next.js",
    "geist-ui"
  ],
  "author": "@accretence",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Accretence/Aryana/issues"
  },
  "homepage": "https://github.com/Accretence/Aryana#readme",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "unpkg": "dist/index.min.js",
  "scripts": {
    "dev": "next dev",
    "stop": "taskkill -F -IM node.exe",
    "update": "npx npm-check-updates -u",
    "prettier": "prettier --write .",
    "test": "jest --config .jest.config.js --no-cache",
    "test:update": "jest --config .jest.config.js --no-cache --update-snapshot",
    "test:coverage": "npm run test --coverage",
    "build:clear": "rm -rf ./dist ./esm",
    "build:rollup": "rollup -c scripts/rollup.config.js",
    "build:webpack": "webpack --config scripts/webpack.config.js",
    "build:babel": "babel --no-babelrc --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx\" ./src --out-dir ./esm --ignore \"**/__tests__/**/*,**/*.d.ts\"",
    "build:after": "node scripts/move-built-in.js",
    "build": "npm run build:clear && npm run build:rollup && npm run build:babel && npm run build:after"
  },
  "prettier": "@geist-ui/prettier-config",
  "files": [
    "/dist",
    "/esm"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@geist-ui/core": "^2.3.8",
    "@geist-ui/icons": "^1.0.2",
    "@geist-ui/prettier-config": "^1.0.1",
    "@mapbox/rehype-prism": "^0.7.0",
    "@mdx-js/loader": "^2.1.3",
    "@mdx-js/react": "^2.1.3",
    "@next/bundle-analyzer": "^12.2.5",
    "@next/mdx": "^12.2.5",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "axios": "^0.27.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-mediaquery": "^0.1.2",
    "enzyme": "^3.11.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-ts-lambdas": "^1.2.3",
    "eslint-plugin-react": "^7.31.1",
    "extract-mdx-metadata": "^2.0.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.0.0",
    "next": "^12.2.5",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-live": "^2.4.1",
    "react-usestateref": "^1.0.8",
    "rehype-join-line": "^1.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "styled-jsx": "5.0.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-hot-middleware": "^2.25.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.9"
  },
  "peerDependencies": {
    "react": ">=18.0.18",
    "react-dom": ">=18.2.0"
  }
}
